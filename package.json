{
  "name": "micro-timer-spart",
  "description": "Viser hvor mange timer som er spart",
  "version": "2.1.14",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=7.5.0"
  },
  "scripts": {
    "test": "standard && nsp check && tap --reporter=spec test/**/*.js",
    "test-offline": "standard && tap --reporter=spec test/**/*.js",
    "coverage": "tap test/**/*.js --coverage",
    "coveralls": "tap --cov --coverage-report=lcov test/**/*.js && cat coverage/lcov.info | coveralls",
    "setup": "npm install",
    "standard-fix": "standard --fix",
    "start": "micro -p 8080",
    "dev": "micro",
    "now-deploy": "npm test && now --npm --alias"
  },
  "keywords": [
    "microservice",
    "micro",
    "timer spart",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/telemark/micro-timer-spart.git"
  },
  "bugs": {
    "url": "https://github.com/telemarks/micro-timer-spart/issues"
  },
  "now": {
    "aliases": [
      "timer.status.t-fk.win"
    ],
    "env": {
      "NODE_ENV": "production"
    }
  },
  "devDependencies": {
    "coveralls": "2.13.1",
    "nsp": "2.6.2",
    "standard": "10.0.2",
    "tap": "10.2.1"
  },
  "dependencies": {
    "got": "6.7.1",
    "marked": "0.3.6",
    "micro": "7.3.2"
  }
}